<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.audigo.audigo_back.mapper.BoardMapper">

    <select id="selectAllBoard" resultType="hashmap">
        SELECT * 
          FROM board 
    </select>


    <select id="selectOneBoard" resultType="hashmap">
        SELECT * 
          FROM board
         WHERE board_number = #{brdNum}
    </select>

</mapper>